{
    "description": "Practise with italian phrases",
    "manifest_version": 2,
    "name": "Italian practise",
    "version": "0.0.1",

    "background": {
        "scripts": ["background.js"]
    },

    "browser_action": {
        "browser_style": true,
        "default_popup": "popup/popup.html",
        "default_icon": {
            "16": "icons/italy-16.png",
            "32": "icons/italy-32.png"
        }
    },

    "permissions": [
        "https://context.reverso.net/*"
    ]
}
